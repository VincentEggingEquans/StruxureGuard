import win32com.client

try:
    excel = win32com.client.Dispatch("Excel.Application")
    print("✅ Excel COM-object succesvol aangemaakt")

    wb = excel.Workbooks.Add()
    print("✅ Nieuw werkboek aangemaakt")

    wb.Close(False)
    excel.Quit()
    print("✅ Excel succesvol afgesloten")
except Exception as e:
    print("❌ Fout opgetreden:")
    print(e)